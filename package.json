{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "postinstall": "npx simple-git-hooks",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.7.2",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-router": "^1.12.2",
    "axios": "1.6.7",
    "jotai": "^2.6.3",
    "nprogress": "^0.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query-kit": "^3.0.0",
    "usehooks-ts": "^2.10.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@iconify-json/carbon": "^1.1.28",
    "@iconify-json/line-md": "^1.1.33",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@tanstack/router-devtools": "^1.12.2",
    "@tanstack/router-vite-plugin": "^1.12.3",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.6",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "5.0.12",
    "vitest": "^1.2.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
