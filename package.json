{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.32.6",
    "axios": "1.6.2",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-react": "^0.40.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/carbon": "^1.1.19",
    "@iconify-json/line-md": "^1.1.26",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.17.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@unocss/eslint-config": "^0.54.1",
    "@unocss/reset": "^0.54.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "typescript": "^5.1.6",
    "unocss": "^0.54.1",
    "unplugin-auto-import": "^0.16.6",
    "vite": "4.4.9",
    "vitest": "^0.34.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/eslint-config-react",
      "@unocss"
    ]
  }
}
