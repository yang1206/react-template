{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "postinstall": "npx simple-git-hooks",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-router": "^1.31.20",
    "axios": "1.6.8",
    "jotai": "^2.8.0",
    "nprogress": "^0.2.0",
    "qs": "^6.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query-kit": "^3.2.2",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint-react/eslint-plugin": "^1.5.10",
    "@iconify-json/carbon": "^1.1.32",
    "@iconify-json/line-md": "^1.1.37",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@tanstack/router-devtools": "^1.31.20",
    "@tanstack/router-vite-plugin": "^1.31.18",
    "@testing-library/react": "^15.0.6",
    "@types/node": "^20.12.8",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "eslint": "^9.2.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "vite": "5.2.13",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
