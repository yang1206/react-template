{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "postinstall": "npx simple-git-hooks",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-router": "^1.85.0",
    "axios": "1.7.8",
    "jotai": "^2.10.3",
    "nprogress": "^0.2.0",
    "qs": "^6.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query-kit": "^3.3.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint-react/eslint-plugin": "^1.17.1",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/line-md": "^1.2.2",
    "@tanstack/react-query-devtools": "^5.62.0",
    "@tanstack/router-devtools": "^1.85.0",
    "@tanstack/router-vite-plugin": "^1.84.4",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.14.11",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.18.6",
    "vite": "6.0.1",
    "vitest": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
