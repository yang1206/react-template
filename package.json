{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "postinstall": "npx simple-git-hooks",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "axios": "1.11.0",
    "jotai": "^2.13.1",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-query-kit": "^3.3.2",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.52.6",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/line-md": "^1.2.11",
    "@tailwindcss/vite": "4.1.12",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/router-devtools": "^1.131.27",
    "@tanstack/router-vite-plugin": "^1.131.27",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.17.2",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.33.0",
    "eslint-plugin-better-tailwindcss": "^3.7.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "4.1.12",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "vite": "7.1.3",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
