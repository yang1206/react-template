{
  "name": "react-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/react-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.5",
    "axios": "1.4.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@antfu/eslint-config-react": "^0.38.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/line-md": "^1.1.24",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.16.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@unocss/eslint-config": "^0.51.13",
    "@unocss/reset": "^0.51.13",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.2",
    "lint-staged": "^13.2.2",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.15.3",
    "vite": "4.3.6",
    "vitest": "^0.31.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/eslint-config-react",
      "@unocss"
    ]
  }
}
